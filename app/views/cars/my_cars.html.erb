<br>
<div class="container">
  <h1>Your available cars</h1>
  <div class="cards">
    <% @cars.each do |car|%>
    <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://res.cloudinary.com/dxneryfww/image/upload/v1652217993/development/<%=car.photo.key%>.jpg)">
      <%= link_to "#{car.brand} #{car.model}", car_path(car), class: "text-white" %>
    </div>
    <% end %>
  </div>
</div>
<br>
